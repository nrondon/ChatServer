import javax.swing.JFrame;
/**
 * This runs the server
 * 
 * @author Naylene Rondon
 * @version V2
 */
public class SRunner
{
    /**
     * Main
     */
    public static void main(String[]args)
    {
        Server serve = new Server();
        
        serve.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE); //Window Destroyer much
        serve.startRunning();
    }
}
